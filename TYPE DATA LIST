#membuat contoh list

nama = ["Danu", "Ray", "Dari", "Arham", "Ikbal"]

#menamoilkan list dengan perulangan
for i in nama :
	print(i)
	
i = 1
while i < len(nama) :
     print(nama[i])
     i += 1
     
#mengganti atau mengupdate salah satu nilai
nama[1] = "saldi"
print(nama)

#menghapus salah satu list
nama.pop()
print(nama)

del nama[1:3]
print(nama)

nama.remove ("Danu")
print(nama)

#menambah list
nama.append("Danu")
print(nama)

nama.extend(["rey", "deri"])
print(nama)

nama.insert(4, "ikbal")
print(nama)
