#percabangan if

nilai = 100
if (nilai >= 10) :
    print(nilai)


#percabangan if else

nilai = 50
if (nilai >= 40) :
    print("lulus")
else :
    print("tidak lulus")


#percabangan elif

umur = 20
if (umur >18 and umur <30) :
    print("sudah beranjak dewasa")
elif(umur >30 and umur <45) :
     print("masa masa emas")
elif(umur >45 and umur <55) :
     print("masa paruh baya")
elif(umur >= 55) :
     print("masa manula")
else:
     print("masa dibawah umur")
